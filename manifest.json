{
  "name": "Keep Last Two Tabs",
  "version": "0.5",
  "description": "This extension keep last two tabs to prevent Chrome quits when last tab is closed.",
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
            "128": "icon128.png" },
  "background": {
    "scripts": ["background.js"]
  },
  "options_page": "options.html",
  "permissions": [
	"tabs",
	"storage"
	],
  "manifest_version": 2
}
